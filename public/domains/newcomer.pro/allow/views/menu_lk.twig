		<a href="{$ item.url $}" class="lk-menu-list__item {% if item.url == request.path %}lk-menu-list__item--current{% endif %}">
			<div class="lk-menu-list__item-icon">
				{$ item.icon|raw $}
			</div>
			<div class="lk-menu-list__item-title">{$ item.title $}</div>
		</a>
	
	{% endfor %}
</div>